{
	"name": "@vdegenne/vite-plugin-css-modules",
	"version": "0.1.5",
	"type": "module",
	"exports": {
		".": {
			"import": "./lib/index.js",
			"types": "./lib/index.d.ts"
		}
	},
	"scripts": {
		"build": "wireit"
	},
	"files": [
		"./lib/"
	],
	"devDependencies": {
		"@types/node": "^24.8.1",
		"typescript": "^5.9.3",
		"vite": "^7.1.10",
		"wireit": "^0.14.12"
	},
	"wireit": {
		"build": {
			"command": "tsc --pretty",
			"clean": "if-file-deleted",
			"files": [
				"./tsconfig.json",
				"./src"
			],
			"output": [
				"./lib"
			]
		}
	}
}
